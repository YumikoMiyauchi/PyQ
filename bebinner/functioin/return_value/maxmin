# 関数minmaxを定義します
def minmax(x,y,z):
    max = x
    min = x
    for i in [y, z]:
        if i < min:
            min = i
        if i > max:
            max = i
    return min, max
            
            
min1, max1 = minmax(100, 200, 300)
print(min1, max1)

min2, max2 = minmax(444, 33, 222)
print(min2, max2)
